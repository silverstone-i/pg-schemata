{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "include": ["src"],
    "includePattern": ".js$"
  },
  "plugins": [],
  "opts": {
    "destination": "docs",
    "recurse": true,
    "template": "node_modules/docdash"
  },
  "templates": {
    "docdash": {
      "static": true,
      "logo": "images/nap-logo.png",
      "sort": true,
      "search": true,
      "collapse": false,
      "typedefs": true,
      "removeQuotes": "none",
      "scripts": [],
      "menu": {
        "GitHub": {
          "href": "https://github.com/silverstone-i/pg-schemata",
          "target": "_blank",
          "class": "menu-item",
          "id": "github_link"
        }
      }
    }
  }
}
